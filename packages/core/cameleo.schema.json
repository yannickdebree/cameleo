{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "Cameleo workspace configuration",
    "type": "object",
    "properties": {
        "projects": {
            "description": "Projects's workspace list",
            "type": "array"
        },
        "defaultProject": {
            "description": "Name of the default project",
